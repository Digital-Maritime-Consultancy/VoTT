{
    "type": "object",
    "title": "${strings.export.providers.moadJson.displayName}",
    "properties": {
        "assetState": {
            "type": "string",
            "title": "${strings.export.providers.common.properties.assetState.title}",
            "description": "${strings.export.providers.common.properties.assetState.description}",
            "enum": [
                "all",
                "visited",
                "tagged"
            ],
            "default": "visited",
            "enumNames": [
                "${strings.export.providers.common.properties.assetState.options.all}",
                "${strings.export.providers.common.properties.assetState.options.visited}",
                "${strings.export.providers.common.properties.assetState.options.tagged}"
            ]
        },
        "includeLabelImages": {
            "type": "boolean",
            "default": false,
            "title": "${strings.export.providers.common.properties.includeLabelImages.title}",
            "description": "${strings.export.providers.common.properties.includeLabelImages.description}"
        },
        "exportIndividuals": {
            "type": "boolean",
            "default": true,
            "title": "${strings.export.providers.common.properties.exportIndividuals.title}",
            "description": "${strings.export.providers.common.properties.exportIndividuals.description}"
        },
        "includeSegmentAnnotatedImages": {
            "type": "boolean",
            "default": false,
            "title": "${strings.export.providers.common.properties.includeSegmentAnnotatedImages.title}",
            "description": "${strings.export.providers.common.properties.includeSegmentAnnotatedImages.description}"
        }
    }
}
